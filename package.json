{
  "name": "fraudguard-backend",
  "description": "",
  "version": "1.0.0",
  "author": "Christopher Barnes",
  "main": "src/index.ts",
  "test command": "npm t",
  "private": false,
  "scripts": {
    "badge": "npx coverage-badger -e 90 -g 65 -r coverage/clover.xml -d src/assets/",
    "build": "tsc",
    "clean": "npx rimraf ./dist && npx rimraf server.*",
    "docker:build": "docker build -t eu.gcr.io/fraudguard/backend:latest .",
    "docker:push": "docker push eu.gcr.io/fraudguard/backend:latest",
    "linter": "npx eslint src/**/*.ts -c ./.eslintrc.yml --fix",
    "predocker": "npm run build",
    "prettier": "npx prettier --write src/**/*.ts",
    "start:dev": "tsnd --respawn src/index.ts | pino-pretty -c -t",
    "start": "node dist/src/index.js | pino-pretty -c -t",
    "test": "npx jest --detectOpenHandles --coverage",
    "tsc": "npx tsc",
    "docs": "jsdoc -c jsdoc.json"
  },
  "keywords": [
    "Node",
    "npm",
    "Express",
    "TypeScript",
    "ESLint",
    "REST",
    "MongoDB",
    "mongoose",
    "CORS",
    "Jest",
    "ts-jest",
    "fetch",
    "Chai"
  ],
  "license": "GNU-GPL3",
  "git repository": "https://github.com/Barnes-C/FraudGuard-Backend",
  "homepage": "https://www.barnes.biz",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "files": [
    "dist",
    "package.json",
    ".env",
    ".gitignore",
    ".npmrc",
    "tsconfig.json",
    "scripts"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "axios": "^0.21.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint": "^7.25.0",
    "express": "^4.17.1",
    "express-bearer-token": "^2.4.0",
    "express-pino-logger": "^6.0.0",
    "express-rate-limit": "^5.2.6",
    "helmet": "^4.4.1",
    "ip": "^1.1.5",
    "json5": "^2.2.0",
    "mongoose": "^5.12.10",
    "pino": "^6.11.1",
    "pino-pretty": "^4.5.0",
    "release": "^6.3.0",
    "strip-indent": "^3.0.0",
    "swagger-ui-express": "^4.1.6",
    "type": "^2.3.0"
  },
  "devDependencies": {
    "@jest/globals": "^26.6.2",
    "@types/chai": "^4.2.17",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-pino-logger": "^4.0.2",
    "@types/express-rate-limit": "^5.1.1",
    "@types/ip": "^1.1.0",
    "@types/node-fetch": "^2.5.10",
    "@types/pino": "^6.3.6",
    "@types/rimraf": "^3.0.0",
    "@types/shelljs": "^0.8.8",
    "@types/uuid": "^8.3.0",
    "better-docs": "^2.3.2",
    "chai": "^4.3.4",
    "copyfiles": "^2.4.1",
    "coverage-badger": "^1.0.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.1.0",
    "husky": "^5.1.2",
    "jest-circus": "^26.6.3",
    "jest-coverage-shield": "^1.1.2",
    "jest-each": "^26.6.2",
    "jest-junit": "^12.0.0",
    "jsdoc": "^3.6.7",
